class Student {
    private String name;
    private int roll;

    Student(String n, int r) {
        name = n;
        roll = r;
    }

    void delete() {
        name = null;
        roll = 0;
    }

    void Getter() {
        System.out.println("Name: " + name + "\nRoll: " + roll);
    }
}

public class StudentDelete {
    public static void main(String[] args) {
        Student s = new Student("Rohan", 101);
        s.delete();
        s.Getter();
    }
}
