class Student {
    private String name;
    private int roll;

    Student(String name, int roll) {
        this.name = name;
        this.roll = roll;
    }

    void Getter() {
        System.out.println("Name: " + name + "\nRoll: " + roll);
    }
}

public class StudentThisKeyword {
    public static void main(String[] args) {
        Student s = new Student("Rohan", 101);
        s.Getter();
    }
}
