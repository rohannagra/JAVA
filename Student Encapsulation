class Student {
    private String name;
    private int roll;

    public Student(String n, int r) {
        name = n;
        roll = r;
    }

    public void Getter() {
        System.out.println("Name: " + name + "\nRoll: " + roll);
    }
}

public class StudentEncapsulation {
    public static void main(String[] args) {
        Student s = new Student("Rohan", 101);
        s.Getter();
    }
}
