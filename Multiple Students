class Student {
    private String name;
    private int roll;

    Student(String n, int r) {
        name = n;
        roll = r;
    }

    void Getter() {
        System.out.println("Name: " + name + "\nRoll: " + roll);
    }
}

public class MultipleStudents {
    public static void main(String[] args) {
        Student[] s = {
            new Student("Rohan", 101),
            new Student("Aryan", 102)
        };
        for (Student st : s)
            st.Getter();
    }
}
